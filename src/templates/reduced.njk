<!DOCTYPE html>
<html>
    <head>
        {% include "elements/head.njk" %}
    </head>
    <body class="styleguide-single">

        {% include "elements/svgmap.njk" %}

        {% include "elements/navigation.njk" %}


        <script src="{{ wayToRoot }}{{ webPath }}js/sg-priority.js" ></script>
        <main class="sg-main">
            <div class="nav-toggle-helper transform-this"></div>
            {% import "single.njk" as singleView %}
            {#{{ singleView.render( element, webPath=webPath ~ '/', iframe = true, iframeLazyload = false,#}
            {{ singleView.render( element, webPath=webPath, iframe = true, iframeLazyload = false,
            wayToRoot = wayToRoot, setSrcLinkOnElementName = true ) }}
        </main>

        {% include "elements/prism.njk" %}

        <script src="{{ wayToRoot }}{{ webPath }}js/sg-scripts.js" ></script>

        {% for script in footerScripts %}
            <script src="{{ script }}"></script>
        {% endfor %}

        {#<script>iFrameResize( {checkOrigin: false, log: true}, 'iframe' );</script>#}

    </body>
</html>
