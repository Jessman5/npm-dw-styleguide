{
  "name": "npm-dw-styleguide",
  "version": "2.0.3",
  "description": "automatically generate a static styleguide from html pattern",
  "repository": "https://github.com/denkwerk/npm-dw-styleguide.git",
  "main": "index.js",
  "keywords": [
    "pattern",
    "pattern-library",
    "styleguide",
    "living"
  ],
  "author": "Erik Marstaeller",
  "license": "MIT",
  "bin": {
    "dw-styleguide": "bin/dw-styleguide"
  },

  "scripts": {
    "lint": "eslint generate/** src/js/sg-scripts-content-btns/** src/js/sg-scripts-navi-iframe/**",
    "lint-fix": "eslint generate/** src/js/sg-scripts-content-btns/** src/js/sg-scripts-navi-iframe/** --fix"
  },

  "dependencies": {
    "concat": "^1.0.3",
    "copy": "^0.3.0",
    "glob": "^7.1.0",
    "nopt": "^3.0.6",
    "nunjucks": "^2.5.2",
    "showdown": "^1.4.4"
  },
  "devDependencies": {
    "eslint": "^4.12.0"
  }
}
