<title>{{ title }}</title>

<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">

{# https://github.com/sindresorhus/github-markdown-css #}
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/github-markdown-css/2.4.1/github-markdown.min.css">

{# Style Guide Styles #}
<link rel="stylesheet" href="{{ webPath }}/css/sg-style.css">
<!--[if lt IE 9]>
<link rel="stylesheet" href="{{ webPath }}/css/sg-style-old-ie.css"><![endif]-->

{# Pattern & Module Styles #}
{% for styles in stylesheets %}
    <link rel="stylesheet" href="{{ styles }}"/>
{% endfor %}

        {# prism Syntax Highlighting Styles #}
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/prism/1.5.1/themes/prism.min.css">


<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>

<script src="{{ webPath }}/js/vendor/iframeResizer.min.js" ></script>
<script src="{{ webPath }}/js/vendor/lazysizes.min.js"></script>


{% for script in headerScripts %}
    <script src="{{ script }}"></script>
{% endfor %}

{{ headEndCode | safe }}